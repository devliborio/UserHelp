<%- include ('../../partials/header.ejs') %>
<body>
    <%- include ('../../partials/homenavbar.ejs')%>

    <br>
    <div class="container">
        <br>
        <% result.articles.rows.forEach((article) => { %>

            <div class="card">

                <div class="card-header">
                    <p class="h5"><%= article.title %></p>
                </div>

                <div class="card-body">
                    <a class="btn btn-success" href="/<%= article.slug %>">Ler artigo</a>
                </div>

            </div>
            <br>
        <% }) %>
    </div>

</body>
